{
  "name": "testgpt",
  "displayName": "TestGPT",
  "description": "Automatic unit testing for your files using GPT-3.5-turbo model",
  "version": "0.0.2",
  "publisher": "FayezNazzal",
  "repository": {
    "type": "git",
    "url": "https://github.com/fayez-nazzal/testgpt"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "@types/glob": "^8.1.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  },
  "contributes": {
    "commands": [
      {
        "command": "testgpt.runTestGpt",
        "title": "Run TestGPT for this file"
      }
    ],
    "configuration": [
      {
        "title": "TestGPT",
        "properties": {
          "testgpt.apiKey": {
            "type": "string",
            "default": "",
            "description": "API Key for TestGPT"
          },
          "testgpt.model": {
            "type": "string",
            "default": "gpt-3.5-turbo",
            "description": "GPT Model to use for TestGPT"
          }
        }
      }
    ]
  }
}
